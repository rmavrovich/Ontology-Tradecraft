PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?label ?entity1 ?entity2
WHERE {
  ?entity1 rdfs:label ?label .
  ?entity2 rdfs:label ?label .

  # Ensure two distinct entities
  FILTER( ?entity1 != ?entity2 )
}
ORDER BY ?label

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?lexicalLabel ?entity1 ?entity2
WHERE {
  ?entity1 rdfs:label ?l1 .
  ?entity2 rdfs:label ?l2 .

  FILTER( ?entity1 != ?entity2 )

  # Compare lexical forms only
  BIND( STR(?l1) AS ?lexicalLabel )
  FILTER( STR(?l1) = STR(?l2) )
}
ORDER BY ?lexicalLabel